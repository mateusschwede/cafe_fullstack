        "vue-router": "^4.5.1"
    },
    "devDependencies": {
        "@cypress/webpack-dev-server": "^4.1.0",
        "@tsconfig/node22": "^22.0.2",
        "@types/node": "^22.15.32",
        "@vitejs/plugin-vue": "^6.0.0",
        "@vitest/ui": "^3.2.4",
        "@vue/test-utils": "^2.4.6",
        "@vue/tsconfig": "^0.7.0",
        "axios": "^1.10.0",
        "bootstrap": "^5.3.7",
        "bootstrap-vue-next": "^0.30.4",
        "cypress": "^14.5.2",
        "jsdom": "^26.1.0",
        "npm-run-all2": "^8.0.4",
        "ts-node": "^10.9.2",
        "typescript": "~5.8.0",
        "vite": "^7.0.0",
        "vite-plugin-vue-devtools": "^7.7.7",
        "vitest": "^3.2.4",
        "vue-tsc": "^2.2.10"
